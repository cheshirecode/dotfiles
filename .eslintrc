{
  "env": {
    "browser": true,
    "es6": true
  },
  "globals": {
    "process": true,
    "module": true,
    "window": true,
    "require": true
  },
  "extends": [
    "eslint:recommended",
    "google",
    "plugin:react/recommended",
    "plugin:jsx-a11y/recommended",
    "prettier",
    "prettier/react"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "module"
  },
  "plugins": [
    "babel",
    "react",
    "jsx-a11y",
    "prettier"
  ]
}
